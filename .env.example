# OllamaサーバーのベースURLを指定
OLLAMA_BASE_URL="http://localhost:11434"

# Ollamaで使用するモデル名を指定 (例: llama3, mistral, phi3 など)
OLLAMA_MODEL="qwen3:14b"

# ALLOW_DANGEROUS_CODEは、AIエージェントが危険なコード（例: ファイル操作、システムコマンドの実行など）を実行できるようにする設定です。
# Trueに設定すると、セキュリティリスクが高まるため、十分注意して使用してください。
ALLOW_DANGEROUS_CODE=True

# Whisper API endpoint
WHISPER_URL="http://localhost:8000/v1/audio/transcriptions"
WHISPER_MODEL="whisper-large-v3-turbo"
